        <Button x:Name="checkButton" Content="Check" HorizontalAlignment="Center" Margin="0,196,0,0" VerticalAlignment="Top" Width="136" Height="33" BorderBrush="#FF00DF03" Background="#FF00DF03" Foreground="White"/>

